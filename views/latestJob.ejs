<% if(status==true){ %>
    <%- include('partials/redirectheader'); %>
        <% } else{ %>

            <%- include('partials/header'); %>
                <% } %>


                    <div class="container">
                        <div class="inner-container">
                            <div class="row">
                                <% latestJobArray.forEach((latestJob)=>{ %>
                                    <div class="card">
                                        <div class="card-body">
                                            <h5 class="card-title">
                                                <%= latestJob.title %>
                                            </h5>
                                            <p class="card-text">Starting Date :
                                                <%= latestJob.startingDate %> <br> Last Date :
                                                    <%= latestJob.lastDate  %><br> Eligiblity :
                                                        <%= latestJob.eligiblity  %> <br> Vacancy :
                                                            <%= latestJob.vacancy %>
                                            </p>
                                            <a href=<%=latestJob.applyLink %> class="btn btn-primary">Apply</a>
                                            <a href=<%=latestJob.descriptionLink %>  class="btn btn-secondary btn-description">See Description</a>
                                        </div>
                                    </div>
                                    <% }) %>


                            </div>
                        </div>
                    </div>

                    <%- include('partials/footer') %>