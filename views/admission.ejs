<% if(status==true){ %>
    <%- include('partials/redirectheader'); %>
        <% } else{ %>

            <%- include('partials/header'); %>
                <% } %>


                    <div class=" row container-fluid">
                        <% Array.forEach((admission)=>{ %>
                            <div class="col col-md-4 col-sm-6 col-12 col-lg-3">
                                <div class="card">
                                    <img class="card-img-top" src="/images/college-<%=admission.id %>.jpg" alt="Card image cap" style="height: 200px;">
                                    <div class="card-body">
                                        <h5 class="card-title">
                                            <%= admission.title %>
                                        </h5>
                                        <p class="card-text">
                                            <%= admission.description %><br> Admission Start :
                                                <%= admission.startingDate %> <br>Admission Close :
                                                    <%= admission.lastDate %>
                                        </p>
                                        <a href=<%=admission.descriptionLink %> class="btn btn-primary">See Description</a>
                                    </div>
                                </div>
                            </div>
                            <% }) %>

                    </div>
                    <%- include('partials/footer') %>